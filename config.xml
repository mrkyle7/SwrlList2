<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="10009" id="co.swrl.List2" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Swrl List 2</name>
    <description>
        Remember and recommend what you should watch, read or listen to
    </description>
    <author email="kylejharrison@gmail.com" href="https://swrl-list.herokuapp.com/">
        Kyle Harrison
    </author>
    <platform name="android">
        <icon src="res/icon/icon.png" />
    </platform>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-webpack" spec="0.0.2" />
    <universal-links>
        <host name="swrl1118.page.link" scheme="https" />
        <host name="swrl-1118.firebaseapp.com" scheme="https">
            <path url="/__/auth/callback" />
        </host>
    </universal-links>
    <preference name="AndroidLaunchMode" value="singleInstance" />
    <preference name="android-minSdkVersion" value="19" />
    <plugin name="cordova-universal-links-plugin" spec="https://github.com/walteram/cordova-universal-links-plugin.git" />
    <plugin name="cordova-plugin-buildinfo" spec="2.0.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="3.0.0" />
    <plugin name="cordova-plugin-customurlscheme" spec="4.3.0">
        <variable name="URL_SCHEME" value="co.swrl.List2" />
        <variable name="ANDROID_SCHEME" value=" " />
        <variable name="ANDROID_HOST" value=" " />
        <variable name="ANDROID_PATHPREFIX" value="/" />
    </plugin>
    <platform name="android">
        <hook src="scripts/afterAddBrowserTabPlugin.js" type="after_plugin_add" />
    </platform>
    <plugin name="cordova-plugin-browsertab" spec="0.2.0" />
    <engine name="ios" spec="^4.5.5" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.1.4" />
</widget>
